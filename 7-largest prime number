// 7.	Write a program to find the largest prime factor of a given number N. Analyze its time complexity
#include <stdio.h>

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    int i = 2;
    int largest = 0;

    while (n > 1) {
        if (n % i == 0) {
            largest = i;
            n = n / i;
        } else {
            i++;
        }
    }

    printf("Largest prime factor is %lld\n", largest);
    return 0;
}
